<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/layout_bg"
        tools:context=".MainActivity">

        <ImageView
            android:id="@+id/iv_1"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.437" />

        <ImageView
            android:id="@+id/iv_3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/iv_1"
            app:layout_constraintEnd_toStartOf="@id/iv_5"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.437" />

        <ImageView
            android:id="@+id/iv_2"
            android:layout_width="50dp"
            android:layout_height="50dp"
            app:layout_constraintEnd_toStartOf="@id/iv_3"
            app:layout_constraintStart_toEndOf="@id/iv_1"
            app:layout_constraintBottom_toTopOf="@id/iv_1" />

        <ImageView
            android:id="@+id/iv_6"
            android:layout_width="50dp"
            android:layout_height="50dp"
            app:layout_constraintEnd_toStartOf="@id/iv_7"
            app:layout_constraintStart_toEndOf="@id/iv_5"
            app:layout_constraintBottom_toTopOf="@id/iv_5" />

        <ImageView
            android:id="@+id/iv_7"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.437" />

        <ImageView
            android:id="@+id/iv_5"
            android:layout_width="50dp"
            android:layout_height="50dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/iv_3"
            app:layout_constraintEnd_toStartOf="@id/iv_7"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.437" />

        <ImageView
            android:id="@+id/iv_4"
            android:layout_width="50dp"
            android:layout_height="50dp"
            app:layout_constraintEnd_toStartOf="@id/iv_5"
            app:layout_constraintStart_toEndOf="@id/iv_3"
            app:layout_constraintBottom_toTopOf="@id/iv_3" />

        <ImageView
            android:id="@+id/iv_8"
            android:layout_width="50dp"
            android:layout_height="50dp"
            app:layout_constraintEnd_toStartOf="@id/iv_3"
            app:layout_constraintStart_toEndOf="@id/iv_1"
            app:layout_constraintTop_toBottomOf="@id/iv_1" />

        <ImageView
            android:id="@+id/iv_9"
            android:layout_width="50dp"
            android:layout_height="50dp"
            app:layout_constraintEnd_toStartOf="@id/iv_10"
            app:layout_constraintStart_toEndOf="@id/iv_8"
            app:layout_constraintTop_toBottomOf="@id/iv_8" />

        <ImageView
            android:id="@+id/iv_10"
            android:layout_width="50dp"
            android:layout_height="50dp"
            app:layout_constraintEnd_toStartOf="@id/iv_5"
            app:layout_constraintStart_toEndOf="@id/iv_3"
            app:layout_constraintTop_toBottomOf="@id/iv_3" />

        <ImageView
            android:id="@+id/iv_11"
            android:layout_width="50dp"
            android:layout_height="50dp"
            app:layout_constraintEnd_toStartOf="@id/iv_12"
            app:layout_constraintTop_toBottomOf="@id/iv_10"
            app:layout_constraintStart_toEndOf="@id/iv_10" />

        <ImageView
            android:id="@+id/iv_12"
            android:layout_width="50dp"
            android:layout_height="50dp"
            app:layout_constraintEnd_toStartOf="@id/iv_7"
            app:layout_constraintStart_toEndOf="@id/iv_5"
            app:layout_constraintTop_toBottomOf="@id/iv_5" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>